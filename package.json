{
  "name": "PaddleLabel-Frontend",
  "version": "0.1.0",
  "private": true,
  "description": " An effective and flexible tool for data annotation",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider umi build",
    "build:gh-page": "cross-env REACT_APP_ENV=page NODE_OPTIONS=--openssl-legacy-provider umi build",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "playwright": "playwright install && playwright test",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test:open": "npx cypress open",
    "test:run": "npx cypress run",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@ant-design/pro-descriptions": "^1.10.0",
    "@ant-design/pro-form": "^1.52.0",
    "@ant-design/pro-layout": "^6.32.0",
    "@ant-design/pro-table": "^2.61.0",
    "@babel/core": "^7.0.0-0",
    "@types/react-color": "^3.0.6",
    "@umijs/route-utils": "^2.0.0",
    "ahooks": "^3.7.1",
    "antd": "^4.23.5",
    "browserslist": "^4.21.3",
    "classnames": "^2.3.0",
    "dva-core": "^1.6.0-0",
    "jsdom": ">=10.0.0",
    "konva": "^8.3.2",
    "loader-utils": "^3.2.0",
    "lodash": "^4.17.0",
    "minimatch": "^3.0.5",
    "moment": "^2.29.0",
    "omit.js": "^2.0.2",
    "parse-url": "^8.1.0",
    "prop-types": "^15.5.7",
    "rc-field-form": "^1.22.0",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^16.8.4",
    "react-color": "^2.19.3",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^16.8.0",
    "react-helmet-async": "^1.2.0",
    "react-intl": "^5.25.1",
    "react-konva": "^17.0.2-5",
    "react-router": "^5.0.0",
    "recursive-diff": "^1.0.8",
    "redbox-react": "1.x",
    "redux": "4.x",
    "shell-quote": "^1.7.3",
    "umi": "^3.5.0",
    "use-image": "^1.0.8",
    "uuid": "^9.0.0",
    "webpack": "^5.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.8.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "cypress": "12.2.0",
    "detect-installer": "^1.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "express": "^4.17.0",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.5.0",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^25.0.0",
    "swagger-ui-react": "^3.52.0",
    "typescript": "^4.9.4",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}